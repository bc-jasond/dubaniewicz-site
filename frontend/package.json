{
  "name": "filbert",
  "version": "0.0.1",
  "description": "a simple writing platform",
  "main": "index.html",
  "repository": "https://github.com/bc-jasond/filbert.git",
  "author": "Jason Dubaniewicz <jason@dubaniewi.cz>",
  "license": "MIT",
  "private": null,
  "scripts": {
    "clean": "rm -rf dist/*",
    "preflight": "yarn pretty-ci && yarn lint && yarn test",
    "test": "jest --collectCoverage=false",
    "test-watch": "jest --bail --notify --watchAll",
    "test-coverage": "jest --coverage",
    "build-prod": "webpack --config webpack-prod.config.js -p --mode production",
    "start": "webpack-dev-server --hot --mode development",
    "start-prod": "webpack-dev-server --config webpack-prod.config.js -p --mode production",
    "start-prod-local": "USE_LOCALHOST_API=true webpack-dev-server --hot --mode production",
    "pretty": "prettier --parser=babel --single-quote --write \"**/*.{js,jsx}\"",
    "pretty-ci": "prettier --parser=babel --single-quote --check \"**/*.{js,jsx}\"",
    "lint": "eslint --fix \"**/*.{js,jsx}\"",
    "nibble": "eslint-nibble \"**/*.{js,jsx}\""
  },
  "sideEffects": false,
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.4",
    "express": "^4.17.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "immutable": "^4.0.0-rc.12",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "set-value": "^3.0.1",
    "styled-components": "^4.4.0",
    "svg-react-loader": "^0.4.6",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-nibble": "^5.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^4.12.18",
    "webpack-dev-server": "^3.1.14"
  }
}
